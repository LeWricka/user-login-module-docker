{
    "name": "deg540/docker-php-boilerplate",
    "description": "Docker PHP testing boilerplate",
    "type": "project",
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "UserLoginService\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UserLoginService\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1"
    },
    "scripts": {
        "test": [
            "phpunit --configuration phpunit.xml"
        ]
    }
}
